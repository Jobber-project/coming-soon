{
  "name": "coming-soon",
  "version": "1.0.0",
  "private": true,
  "description": "Jobello",
  "author": "Jesper Johansson",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^2.32.3",
    "gatsby-plugin-apollo": "^3.0.2",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-no-index": "^1.0.2",
    "gatsby-plugin-node-reload": "^1.1.0",
    "gatsby-plugin-portal": "^1.0.7",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "isomorphic-fetch": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dom-confetti": "^0.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.15.3",
    "react-is": "^16.8.0",
    "styled-components": "^5.2.1",
    "styled-reset": "^4.3.4"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-jobber": "git+ssh://git@github.com:jobber-project/eslint-config-jobber.git",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": "jobber"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*]).js": [
      "eslint",
      "prettier --write"
    ]
  }
}
