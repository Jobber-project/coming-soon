{
  "name": "coming-soon",
  "version": "1.0.0",
  "private": true,
  "description": "Jobelo",
  "author": "Jesper Johansson",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "gatsby": "^2.32.3",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-jobber": "git+ssh://git@github.com:jobber-project/eslint-config-jobber.git",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": "jobber"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*]).js": [
      "eslint",
      "prettier --write"
    ]
  }
}
